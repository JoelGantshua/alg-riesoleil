{
  "headers": [
    {
      "source": "/(.*)\\.(js|css|json|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/logo192.png", "destination": "/logo192.png" },
    { "source": "/logo512.png", "destination": "/logo512.png" },
    { "source": "/screenshots/(.*)", "destination": "/screenshots/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
